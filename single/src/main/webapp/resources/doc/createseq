-- 그룹 테스트 코드
-- 그룹 seq
drop sequence group_num_seq;
-- 그룹 카테고리 seq
drop sequence group_category_num_seq;

-- 그룹 일정 seq
drop sequence calendar_num_seq;


-- 그룹 숙제 seq
drop sequence homework_num_seq;


-- 그룹 숙제 진행상황 seq
drop sequence group_hprogress_num_seq;


--해시태그 타입 seq
drop sequence hashtag_type_num_seq;


--해시태그 seq
drop sequence hashtag_num_seq;


-- 시퀀스 삭제
drop sequence category_num_seq;
drop sequence board_type_num_seq;
drop sequence board_list_num_seq;
drop sequence board_num_seq;
drop sequence reply_num_seq;



-----------------------------------------------------------------------


create sequence group_num_seq
start with 100
increment by 1
minvalue 0
;
-- 그룹 카테고리 seq
create sequence group_category_num_seq
start with 100
increment by 1
minvalue 0
;

-- 그룹 일정 seq
create sequence calendar_num_seq
start with 100
increment by 1
minvalue 0
;

-- 그룹 숙제 seq
create sequence homework_num_seq
start with 100
increment by 1
minvalue 0
;

-- 그룹 숙제 진행상황 seq
create sequence group_hprogress_num_seq
start with 100
increment by 1
minvalue 0
;

--해시태그 타입 seq
create sequence hashtag_type_num_seq
start with 100
increment by 1
minvalue 0
;

--해시태그 seq
create sequence hashtag_num_seq
start with 100
increment by 1
minvalue 0
;


-- 게시판 카테고리
create sequence category_num_seq increment by 1 start with 100 minvalue 0;
-- 게시판 형식
create sequence board_type_num_seq increment by 1 start with 100 minvalue 0;
-- 게시판 리스트
create sequence board_list_num_seq increment by 1 start with 100 minvalue 0;
-- 게시판
create sequence board_num_seq increment by 1 start with 100 minvalue 0;
-- 댓글
create sequence reply_num_seq increment by 1 start with 100 minvalue 0;







		